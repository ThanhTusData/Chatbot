{
  "dashboard": {
    "title": "Model Performance Metrics",
    "panels": [
      {
        "title": "Predictions by Intent",
        "targets": [
          {
            "expr": "rate(chatbot_predictions_total[5m])"
          }
        ],
        "type": "graph"
      },
      {
        "title": "Prediction Confidence",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(chatbot_prediction_confidence_bucket[5m]))"
          }
        ],
        "type": "graph"
      },
      {
        "title": "Average Confidence by Intent",
        "targets": [
          {
            "expr": "avg(chatbot_prediction_confidence) by (intent)"
          }
        ],
        "type": "graph"
      },
      {
        "title": "Model Inference Time",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(model_inference_seconds_bucket[5m]))"
          }
        ],
        "type": "graph"
      }
    ]
  }
}